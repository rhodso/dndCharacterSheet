    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<dndCS>",
            "request": "launch",
            "mainClass": "com.rhodso.app.App",
            "projectName": "dndCS"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-MainMenu<dndCS>",
            "request": "launch",
            "mainClass": "com.rhodso.app.MainMenu",
            "projectName": "dndCS"
        },
        {
            "type": "java",
            "name": "Make Jar",
            "request": "launch",
            "mainClass": "com.rhodso.app.App",
            "projectName": "dndCS",
            "preLaunchTask": {
                "task": "mkPkg",
                "type": "shell"
            }
        }
    ]
}